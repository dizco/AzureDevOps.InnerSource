{
  "IdentityProvider": {
    "Authority": "https://login.microsoftonline.com/<azure ad tenant id here>/v2.0/",
    "ClientId": "<client id here>",
    "ClientSecret": "<client secret here>"
  },
  "DevOps": {
    "Organization": "<organization name here>",
    "StarsAllowedRepositories": [
      {
        "RegexProject": "(.*?)",
        "RegexRepository": "(.*?)"
      }
    ]
  },
  "Storage": {
    "Mode": "TableStorage",
    "TableStorageConnectionString": "<connection string here>",
    "TableName": "azuredevopsstars"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
